{
    "name": "d3-fluid",
    "description": "Reactive data visualization components",
    "version": "0.1.1",
    "author": {
        "name": "quantmind.com",
        "email": "message@quantmind.com"
    },
    "keywords": [],
    "license": "BSD-3-Clause",
    "repository": {
        "type": "git",
        "url": "https://github.com/quantmind/d3-fluid.git"
    },
    "engines": {
        "node": ">= 6.0"
    },
    "main": "build/d3-fluid.min.js",
    "module": "index",
    "jsnext:main": "index",
    "dependencies": {
        "crossfilter": "1.3",
        "d3-axis": "1.0",
        "d3-brush": "1.0",
        "d3-canvas-transition": "0.2",
        "d3-chord": "1.0",
        "d3-dsv": "1.0",
        "d3-force": "1.0",
        "d3-format": "1.0",
        "d3-hexbin": "0.2",
        "d3-hierarchy": "1.0",
        "d3-let": "0.2",
        "d3-path": "1.0",
        "d3-scale": "1.0",
        "d3-shape": "1.0",
        "d3-time-format": "2.0",
        "d3-zoom": "1.1",
        "d3-view": "0.1"
    },
    "devDependencies": {
        "babel-eslint": "7.1",
        "babel-plugin-transform-async-to-generator": "6.16",
        "babel-preset-es2015": "6.18",
        "babel-preset-es2015-rollup": "rollup/babel-preset-es2015-rollup",
        "babel-polyfill": "6.16",
        "babelify": "7.3",
        "browserify": "13.1",
        "browserify-istanbul": "2.0",
        "concat-files": "0.1",
        "eslint": "3.11",
        "handlebars": "4",
        "istanbul": "0.4",
        "karma": "1.3",
        "karma-browserify": "5.1",
        "karma-coverage": "1.1",
        "karma-chrome-launcher": "2.0",
        "karma-firefox-launcher": "1.0",
        "karma-phantomjs-launcher": "1.0",
        "karma-junit-reporter": "1.2",
        "karma-es5-shim": "0.0.4",
        "karma-jasmine": "1.0",
        "karma-safari-launcher": "1.0",
        "jasmine-core": "2.5",
        "package-preamble": "0.0",
        "phantomjs-prebuilt": "2.1",
        "publish": "0.6",
        "requirejs": "2.3",
        "rollup": "0.36",
        "rollup-plugin-babel": "2.6",
        "rollup-plugin-commonjs": "5.0",
        "rollup-plugin-json": "2.0",
        "rollup-plugin-node-resolve": "2.0",
        "uglify-js": "2.7",
        "watchify": "3.7",
        "whatwg-fetch": "2.0"
    },
    "scripts": {
        "test": "eslint index.js rollup.config.js test src && npm run-script unit",
        "rollup": "rollup -c --banner \"$(preamble)\"",
        "cover": "karma start test/karma/karma.cover.js",
        "minify": "uglifyjs --preamble \"$(preamble)\" build/d3-fluid.js -c -m -o build/d3-fluid.min.js",
        "build": "npm run-script rollup && npm run-script minify",
        "unit": "karma start test/karma/karma.unit.js",
        "debug": "karma start test/karma/karma.debug.js",
        "prepublish": "npm run-script build",
        "postpublish": "VERSION=`node -e 'console.log(require(\"./package.json\").version)'`; git push && git tag -am \"Release $VERSION.\" v${VERSION} && git push --tags && mkdir -p ../giottojs.org/latest && mkdir -p ../giottojs.org/d3-fluid/${VERSION} && cp build/* ../giottojs.org/d3-fluid/${VERSION}/ && cp build/* ../giottojs.org/latest/ && cd ../giottojs.org && git add d3-fluid/${VERSION} latest && git commit -a -m \"d3-fluid ${VERSION}\" && git push",
        "release": "publish"
    }
}
